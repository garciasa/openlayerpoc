{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Template for React App",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --inline --history-api-fallback",
    "test": "mocha --opts mocha.opts --require ./src/test/utils/test_helper.js",
    "test:watch": "npm run test -- --watch",
    "build:watch": "webpack -p --config webpack.config.js --progress --colors --watch",
    "build:dev": "node ./node_modules/webpack/bin/webpack -p --config webpack.config.js --progress --colors",
    "build:prod": "set NODE_ENV=production&&webpack -p --config webpack.prod.config.js --progress --colors&&npm run copyfiles"
  },
  "author": "garciasa",
  "license": "ISC",
  "dependencies": {
    "history": "^4.4.0",
    "lodash": "^4.16.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "nock": "^9.0.2",
    "node-sass": "^4.5.0",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
